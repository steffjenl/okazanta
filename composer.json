{
  "name": "cachethq/cachet",
  "description": "An open source status page system, for everyone.",
  "keywords": [
    "laravel",
    "cachet",
    "alt-three",
    "status",
    "page"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "James Brooks",
      "email": "james@alt-three.com"
    },
    {
      "name": "Graham Campbell",
      "email": "graham@alt-three.com"
    },
    {
      "name": "Joseph Cohen",
      "email": "joe@alt-three.com"
    }
  ],
  "replace": {
    "paragonie/random_compat": "*",
    "symfony/polyfill-mbstring": "*"
  },
  "require": {
    "php": "^8.1",
    "bugsnag/bugsnag-laravel": "*",
    "chillerlan/php-qrcode": "*",
    "doctrine/dbal": "*",
    "graham-campbell/binput": "*",
    "graham-campbell/exceptions": "*",
    "graham-campbell/markdown": "*",
    "guzzlehttp/guzzle": "*",
    "jenssegers/date": "*",
    "laravel/framework": "^10.10",
    "laravel/helpers": "*",
    "laravel/legacy-factories": "*",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "mccool/laravel-auto-presenter": "*",
    "pragmarx/google2fa": "^8.0",
    "predis/predis": "*",
    "spatie/icalendar-generator": "*",
    "spatie/laravel-feed": "*",
    "steffjenl/badger": "^2.0@dev",
    "steffjenl/bus": "4.5.x-dev",
    "steffjenl/emoji": "^3.0@dev",
    "steffjenl/twitter": "^3.0@dev",
    "steffjenl/validator": "4.5.x-dev",
    "twig/twig": "^3.7"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "*",
    "fakerphp/faker": "^1.9.1",
    "filp/whoops": "^2.15",
    "graham-campbell/analyzer": "^4.0",
    "laravel/pint": "^1.0",
    "laravel/sail": "*",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.1",
    "spatie/laravel-ignition": "^2.0",
    "steffjenl/testbench": "4.3.x-dev"
  },
  "suggest": {
    "ext-apc": "APC Support cache driver."
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "CachetHQ\\Cachet\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CachetHQ\\Tests\\Cachet\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "test": [
      "@php artisan config:clear",
      "vendor/bin/phpunit"
    ],
    "test-coverage": [
      "@php artisan config:clear",
      "vendor/bin/phpunit --coverage-clover=coverage.xml"
    ]
  },
  "config": {
    "platform": {
      "php": "8.1.21"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.4-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/steffjenl/Twitter.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/steffjenl/Emoji.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/steffjenl/Badger.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/steffjenl/TestBench.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/steffjenl/Bus.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/steffjenl/Validator.git"
    },
      {
        "type": "vcs",
        "url": "https://github.com/laravel-shift/laravel-auto-presenter.git"
      }
  ]
}
